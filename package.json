{
  "name": "color-lut",
  "private": "true",
  "version": "1.0.0",
  "description": "Lut processor for images.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "concurrently -m 4 'pnpm:build:*'",
    "build:base": "rollup -c",
    "build:middleware": "rollup -c --config-middleware",
    "pretest": "tsc --noEmit",
    "test": "ts-node src/fetch-lut.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leezhian/color-lut.git"
  },
  "keywords": [
    "lut",
    "luts",
    "filter"
  ],
  "sideEffects": false,
  "author": "kim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leezhian/color-lut/issues"
  },
  "homepage": "https://github.com/leezhian/color-lut",
  "files": [
    "**"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "module": "./esm/index.js",
      "default": "./index.js"
    },
    "./middleware": {
      "types": "./middleware.d.ts",
      "module": "./esm/middleware.js",
      "default": "./middleware.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@babel/preset-env": "^7.21.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.1",
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "rollup": "^3.21.6",
    "typescript": "^5.0.4"
  }
}
